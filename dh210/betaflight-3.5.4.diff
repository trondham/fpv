profile 0

set iterm_relax = RP
set p_yaw = 40
set i_yaw = 120
set f_yaw = 100
rateprofile 0

set roll_rc_rate = 90
set pitch_rc_rate = 90
set yaw_rc_rate = 90
set roll_expo = 20
set pitch_expo = 20
set yaw_expo = 20
set roll_srate = 82
set pitch_srate = 82
set yaw_srate = 82

rateprofile 1

set roll_expo = 10
set pitch_expo = 10
set yaw_expo = 10

rateprofile 2

